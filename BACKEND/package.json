{
  "name": "e-commerce-backend",
  "version": "1.0.0",
  "description": "Backend for E-Commerce application",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "data:import": "node utils/seeder.js",
    "data:destroy": "node utils/seeder.js -d",
    "products:import": "node utils/productSeeder.js",
    "products:destroy": "node utils/productSeeder.js -d",
    "products:sample": "node utils/sampleProductSeeder.js",
    "products:sample:destroy": "node utils/sampleProductSeeder.js -d",
    "reviews:sample": "node utils/reviewSeeder.js",
    "reviews:sample:destroy": "node utils/reviewSeeder.js -d"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0",
    "morgan": "^1.10.0",
    "stripe": "^14.5.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}